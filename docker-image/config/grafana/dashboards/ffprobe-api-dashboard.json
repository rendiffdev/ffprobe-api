{
  "dashboard": {
    "id": null,
    "title": "FFprobe API - Production Dashboard",
    "tags": ["ffprobe", "video", "api", "production"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "refresh": "30s",
    "version": 1,
    "panels": [
      {
        "id": 1,
        "title": "System Overview",
        "type": "stat",
        "gridPos": {"h": 4, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(up{job=\"ffprobe-api\"})",
            "legendFormat": "API Instances"
          },
          {
            "expr": "rate(http_requests_total{job=\"ffprobe-api\"}[5m])",
            "legendFormat": "Request Rate"
          },
          {
            "expr": "ffprobe:error_rate_5m * 100",
            "legendFormat": "Error Rate %"
          },
          {
            "expr": "ffprobe:response_time_95p_5m * 1000",
            "legendFormat": "95th Response Time (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"displayMode": "list", "orientation": "horizontal"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 80}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Request Rate & Response Time",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"ffprobe-api\"}[5m])) by (instance)",
            "legendFormat": "{{instance}} - Requests/sec"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket{job=\"ffprobe-api\"}[5m])) by (le, instance)) * 1000",
            "legendFormat": "{{instance}} - 95th percentile (ms)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "custom": {"drawStyle": "line", "lineInterpolation": "linear", "barAlignment": 0, "lineWidth": 1, "fillOpacity": 10, "gradientMode": "none", "spanNulls": false, "insertNulls": false, "showPoints": "never", "pointSize": 5, "stacking": {"mode": "none", "group": "A"}, "axisPlacement": "auto", "axisLabel": "", "scaleDistribution": {"type": "linear"}, "hideFrom": {"legend": false, "tooltip": false, "vis": false}, "thresholdsStyle": {"mode": "off"}},
            "mappings": [],
            "thresholds": {"mode": "absolute", "steps": [{"color": "green", "value": null}, {"color": "red", "value": 80}]},
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Error Rate by Status Code",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 4},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"ffprobe-api\",status=~\"4..\"}[5m])) by (status)",
            "legendFormat": "4xx Errors"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"ffprobe-api\",status=~\"5..\"}[5m])) by (status)",
            "legendFormat": "5xx Errors"
          }
        ]
      },
      {
        "id": 4,
        "title": "Video Processing Metrics",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 12},
        "targets": [
          {
            "expr": "ffprobe_concurrent_jobs",
            "legendFormat": "{{instance}} - Concurrent Jobs"
          },
          {
            "expr": "rate(ffprobe_videos_processed_total[5m])",
            "legendFormat": "{{instance}} - Processing Rate"
          },
          {
            "expr": "ffprobe_processing_queue_size",
            "legendFormat": "{{instance}} - Queue Size"
          }
        ]
      },
      {
        "id": 5,
        "title": "Resource Utilization",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 12},
        "targets": [
          {
            "expr": "rate(process_cpu_seconds_total{job=\"ffprobe-api\"}[5m]) * 100",
            "legendFormat": "{{instance}} - CPU %"
          },
          {
            "expr": "process_resident_memory_bytes{job=\"ffprobe-api\"} / 1024 / 1024",
            "legendFormat": "{{instance}} - Memory MB"
          }
        ]
      },
      {
        "id": 6,
        "title": "Cache Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 20},
        "targets": [
          {
            "expr": "rate(cache_hits_total[5m])",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(cache_misses_total[5m])",
            "legendFormat": "Cache Misses/sec"
          },
          {
            "expr": "ffprobe:cache_hit_rate_5m * 100",
            "legendFormat": "Hit Rate %"
          }
        ]
      },
      {
        "id": 7,
        "title": "Quality Analysis Results",
        "type": "stat",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 20},
        "targets": [
          {
            "expr": "sum(ffprobe_quality_checks_total) by (check_type)",
            "legendFormat": "{{check_type}}"
          },
          {
            "expr": "sum(ffprobe_quality_failures_total) by (check_type)",
            "legendFormat": "{{check_type}} Failures"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "custom": {"displayMode": "list", "orientation": "horizontal"},
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 50}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Database & Storage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 28},
        "targets": [
          {
            "expr": "sqlite_database_size_bytes / 1024 / 1024",
            "legendFormat": "SQLite DB Size (MB)"
          },
          {
            "expr": "node_filesystem_avail_bytes{mountpoint=\"/app/data\"} / 1024 / 1024 / 1024",
            "legendFormat": "Available Storage (GB)"
          },
          {
            "expr": "rate(sqlite_operations_total[5m])",
            "legendFormat": "SQLite Ops/sec"
          }
        ]
      },
      {
        "id": 9,
        "title": "Network & Security",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 28},
        "targets": [
          {
            "expr": "rate(auth_failed_attempts_total[5m])",
            "legendFormat": "Failed Auth/sec"
          },
          {
            "expr": "rate(rate_limit_exceeded_total[5m])",
            "legendFormat": "Rate Limits/sec"
          },
          {
            "expr": "active_connections",
            "legendFormat": "Active Connections"
          }
        ]
      },
      {
        "id": 10,
        "title": "AI/LLM Service Performance",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 36},
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(ollama_inference_duration_seconds_bucket[5m]))",
            "legendFormat": "Ollama 95th Inference Time"
          },
          {
            "expr": "rate(ollama_requests_total[5m])",
            "legendFormat": "Ollama Requests/sec"
          },
          {
            "expr": "ollama_active_models",
            "legendFormat": "Active Models"
          },
          {
            "expr": "ollama_memory_usage_bytes / 1024 / 1024 / 1024",
            "legendFormat": "Ollama Memory (GB)"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "allValue": null,
          "current": {"isNone": true, "selected": false, "text": "All", "value": "$__all"},
          "datasource": "Prometheus",
          "definition": "label_values(up{job=\"ffprobe-api\"}, instance)",
          "hide": 0,
          "includeAll": true,
          "label": "Instance",
          "multi": true,
          "name": "instance",
          "options": [],
          "query": "label_values(up{job=\"ffprobe-api\"}, instance)",
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 1,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        }
      ]
    },
    "links": [],
    "gnetId": null,
    "description": "Comprehensive production monitoring dashboard for FFprobe API service including application metrics, infrastructure monitoring, video processing analytics, and business KPIs."
  }
}